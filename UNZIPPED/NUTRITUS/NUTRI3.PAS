  r  = 20;
var
  Saucer    : pointer;
  X, Y      : integer;
  ulx, uly  : word;
  lrx, lry  : word;
  Size      : word;
  I         : word;
begin
  ulx := 20;
  uly := 20;
  lrx := 95;
  lry := 96;
  Baixo('Desculpem, mas tenho que me ir embora ...');
  delay(1000);
  Size := ImageSize(ulx, uly, lrx, lry);
  GetMem(Saucer, Size);
  GetImage(ulx, uly, lrx, lry, Saucer^);
  PutImage(ulx, uly, Saucer^, XORput);
  X := 20;
  Y := 20;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 10;
    X := X + 2;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y - 5;
    X := X + 2;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 5;
    X := X + 2;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y - 3;
    X := X + 2;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 3;
    X := X + 2;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y - 1;
    X := X + 1;
  end;
for I := 1 to 36 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 1;
    X := X + 1;
  end;
Y := Y -3;
for I := 1 to 10 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y - 1;
    X := X + 1;
  end;
for I := 1 to 10 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 1;
    X := X + 1;
  end;
for I := 1 to 50 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    X := X + 3;
  end;
  Baixo('Mas, o programa ainda nÑo acabou, Ç melhor voltar ...');
  delay(2000);
  X := - 40;
  Y := - 40;
for I := 1 to 30 do
  begin
    PutImage(X, Y, Saucer^, XORput);
    PutImage(X, Y, Saucer^, XORput);
    Y := Y + 2;
    X := X + 2;
  end;
  PutImage(X, Y, Saucer^, XORput);
  FreeMem(Saucer, size);
  Baixo('Pressione Enter para a demonstraáÑo');
end;

